<div class="container">

  <div class="row">
    <div class="col-xs-12">
      <table class="table table-bordered" id="results">
        <thead>
          <tr>
            <th>Field</th>
            <th>Value</th>
          <tr>
        </thead>
        <tbody>
      {{#each values}}
          <tr>
            <td id="{{@key}}-name">{{@key}}</td>
            <td id="{{@key}}">{{this}}</td>
          </tr>
      {{/each}}
        </tbody>
      </table>
    </div>
  </div>
</div>